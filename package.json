    "@types/pako": "^1.0.1",
    "@types/terser": "^3.12.0",
    "benchmark": "^2.1.4",
    "fast-memoize": "^2.5.2",
    "filesize": "^6.4.0",
    "glob": "^7.1.7",
    "jest": "^27.0.6",
    "lodash": "^4.17.21",
    "pako": "^2.0.3",
    "prettier": "^2.3.2",
    "rollup": "^2.52.7",
    "tasksfile": "^5.1.1",
    "terser": "^5.7.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
