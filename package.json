{
  "name": "flocky",
  "version": "0.2.8",
  "description": "A TypeScript and JavaScript utility library with clarity and efficiency at the core.",
  "main": "index.js",
  "module": "es6/index.js",
  "author": "queicherius@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queicherius/flocky.git"
  },
  "scripts": {
    "test": "jest --runInBand",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --list-different 'src/**/*.ts'",
    "build": "node tasksfile.js build",
    "task": "node tasksfile.js",
    "generate-docs": "ts-node src/generateDocs.ts",
    "benchmark": "run(){ ts-node src/$1/benchmark.ts; }; run"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/filesize": "^5.0.0",
    "@types/glob": "^7.1.3",
